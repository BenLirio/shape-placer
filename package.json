{
  "name": "socketio-typescript",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "start": "nodemon",
    "build": "tsc && webpack --config webpack.config.js",
    "build:client": "webpack --config webpack.config.js",
    "build:server": "tsc",
    "dev:server": "nodemon --watch src -e ts --exec \"ts-node -r tsconfig-paths/register src/server.ts\"",
    "dev:client": "webpack serve --config webpack.config.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\""
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "chalk": "^5.3.0",
    "express": "^4.19.2",
    "openai": "^4.52.7",
    "p5": "^1.9.4",
    "randomcolor": "^0.6.2",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^14.18.63",
    "@types/p5": "^1.7.6",
    "@types/randomcolor": "^0.5.9",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "concurrently": "^8.2.2",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  }
}
